  "panels": [
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "로그의 생성 시간(event_ts) 기준으로 EPS를 표시합니다. (원본 fact_log 스캔)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "EPS (event_ts)",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "id": 7,
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket,\n  sum(total) / 60 AS eps\nFROM analytics.fact_log_agg_event_1m\nWHERE $__timeFilter(bucket)\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "로그 EPS (event_ts 기준)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "로그 적재 시간(ingest_ts) 기준 EPS입니다. Spark 처리량과 직접 연동됩니다.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "EPS (ingest_ts)",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "id": 8,
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket,\n  sum(total) / 60 AS eps\nFROM analytics.fact_log_agg_1m\nWHERE $__timeFilter(bucket)\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "로그 EPS (ingest_ts 기준)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "event_ts 대비 ingest_ts 지연(초) 평균입니다. (1분 집계)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "Lag (sec)",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 9 },
      "id": 9,
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket AS time,\n  if(cnt = 0, 0, sum_lag / cnt) AS lag_sec\nFROM analytics.fact_log_lag_1m\nWHERE $__timeFilter(bucket)\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "Spark 처리 지연 (avg)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "ClickHouse INSERT 쿼리 지연(p95)입니다. Spark sink 영향도를 보기 위한 패널입니다.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "continuous-GrYlRd" },
          "custom": {
            "axisLabel": "p95 INSERT 지연",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 18 },
      "id": 10,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  toStartOfInterval(event_time, toIntervalSecond($__interval_s)) AS bucket,\n  quantile(0.95)(query_duration_ms) AS p95_ms\nFROM system.query_log\nWHERE $__timeFilter(event_time)\n  AND type = 'QueryFinish'\n  AND startsWith(lowerUTF8(trimLeft(query)), 'insert')\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "ClickHouse INSERT 지연 p95",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "ingest_ts 기준 EPS와 event_ts 기준 EPS의 차이(ingest - event)를 표시합니다.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "EPS gap",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 9 },
      "id": 13,
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  bucket,\n  sum(consumed) - sum(produced) AS eps_gap\nFROM (\n  SELECT\n    bucket,\n    sum(total) / 60 AS produced,\n    0 AS consumed\n  FROM analytics.fact_log_agg_event_1m\n  WHERE $__timeFilter(bucket)\n  GROUP BY bucket\n  UNION ALL\n  SELECT\n    bucket,\n    0 AS produced,\n    sum(total) / 60 AS consumed\n  FROM analytics.fact_log_agg_1m\n  WHERE $__timeFilter(bucket)\n  GROUP BY bucket\n)\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "EPS 차이 (ingest - event)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "description": "MergeTree active parts 수(현재 값). 지속 증가 시 merge backlog 가능성이 큼.",
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 6, "x": 12, "y": 45 },
      "id": 11,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": [ "lastNotNull" ], "fields": "", "values": false }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  now() AS time,\n  count() AS parts\nFROM system.parts\nWHERE active",
          "refId": "A"
        }
      ],
      "title": "Active Parts (now)",
      "type": "stat"
    },
    {
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "description": "현재 진행 중인 merge 개수(현재 값). 높게 유지되면 merge 병목 가능.",
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 6, "x": 18, "y": 45 },
      "id": 12,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": [ "lastNotNull" ], "fields": "", "values": false }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  now() AS time,\n  count() AS merges\nFROM system.merges",
          "refId": "A"
        }
      ],
      "title": "Merge In Progress (now)",
      "type": "stat"
    },
    {
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "description": "event_ts→stored_ts 기준 end-to-end 지연(p95)입니다. (1분 집계)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "p95 지연",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 54 },
      "id": 20,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  time,\n  p95_ms\nFROM (\n  SELECT\n    bucket AS time,\n    quantileTDigestMerge(e2e_state) AS p95_ms\n  FROM analytics.fact_log_latency_1m\n  WHERE $__timeFilter(bucket)\n  GROUP BY bucket\n)\nORDER BY time ASC",
          "refId": "A"
        }
      ],
      "title": "End-to-End 지연 p95",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "description": "ingest/process/sink 구간 지연(p95)입니다. (1분 집계)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "p95 지연",
            "axisPlacement": "auto",
            "axisSoftMin": 0,
            "drawStyle": "line",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 54 },
      "id": 21,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  time,\n  stage,\n  p95_ms\nFROM (\n  SELECT\n    bucket AS time,\n    'ingest' AS stage,\n    quantileTDigestMerge(ingest_state) AS p95_ms\n  FROM analytics.fact_log_latency_1m\n  WHERE $__timeFilter(bucket)\n  GROUP BY bucket\n  UNION ALL\n  SELECT\n    bucket AS time,\n    'process' AS stage,\n    quantileTDigestMerge(process_state) AS p95_ms\n  FROM analytics.fact_log_latency_1m\n  WHERE $__timeFilter(bucket)\n  GROUP BY bucket\n  UNION ALL\n  SELECT\n    bucket AS time,\n    'sink' AS stage,\n    quantileTDigestMerge(sink_state) AS p95_ms\n  FROM analytics.fact_log_latency_1m\n  WHERE $__timeFilter(bucket)\n  GROUP BY bucket\n)\nORDER BY time ASC, stage ASC",
          "refId": "A"
        }
      ],
      "title": "구간 지연 p95 (ingest/process/sink)",
      "type": "timeseries"
    }
  ],
