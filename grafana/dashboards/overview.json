{
  "_comment": "Grafana Log Analytics Overview (ClickHouse fact_log 시계열/서비스별 지표)",
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.4.2"
    },
    {
      "type": "datasource",
      "id": "grafana-clickhouse-datasource",
      "name": "ClickHouse",
      "version": "4.x"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "1"
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": "1"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "target": {
          "limit": 100,
          "matchAny": false,
          "tags": [],
          "type": "dashboard"
        },
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "시간 단위로 fact_log 건수를 집계합니다.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 12,
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "req/min"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  toDateTime(\n    intDiv(\n      toUInt32(toUnixTimestamp(event_ts)),\n      greatest(intDiv($__interval_ms, 1000), 1)\n    ) * greatest(intDiv($__interval_ms, 1000), 1)\n  ) AS bucket,\n  count() AS total\nFROM analytics.fact_log\nWHERE $__timeFilter(event_ts)\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "요청 수 (per $__interval)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
      "description": "서비스별 요청량 추이를 확인합니다.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "showPoints": "never", "spanNulls": true },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] },
          "unit": "req/min"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  toDateTime(\n    intDiv(\n      toUInt32(toUnixTimestamp(event_ts)),\n      greatest(intDiv($__interval_ms, 1000), 1)\n    ) * greatest(intDiv($__interval_ms, 1000), 1)\n  ) AS bucket,\n  service,\n  count() AS total\nFROM analytics.fact_log\nWHERE $__timeFilter(event_ts)\nGROUP BY bucket, service\nORDER BY bucket, service",
          "refId": "A"
        }
      ],
      "title": "서비스별 요청량 추이",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "시간대별 5xx 오류 건수를 모니터링합니다.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          },
          "mappings": [],
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 9 },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  toDateTime(\n    intDiv(\n      toUInt32(toUnixTimestamp(event_ts)),\n      greatest(intDiv($__interval_ms, 1000), 1)\n    ) * greatest(intDiv($__interval_ms, 1000), 1)\n  ) AS bucket,\n  countIf(status_code >= 500) AS errors\nFROM analytics.fact_log\nWHERE $__timeFilter(event_ts)\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "5xx 오류 추이",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "시간대별 평균 지연(ms)",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "continuous-GrYlRd"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          },
          "mappings": [],
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 9 },
      "id": 5,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  toDateTime(\n    intDiv(\n      toUInt32(toUnixTimestamp(event_ts)),\n      greatest(intDiv($__interval_ms, 1000), 1)\n    ) * greatest(intDiv($__interval_ms, 1000), 1)\n  ) AS bucket,\n  round(avg(latency), 2) AS avg_latency_ms\nFROM analytics.fact_log\nWHERE $__timeFilter(event_ts)\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "평균 지연 추이",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "description": "서비스별 평균/P95 지연 상위 10개",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 200
              },
              {
                "color": "red",
                "value": 500
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 18 },
      "id": 6,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right",
          "showLegend": true,
          "values": [ "percent", "value" ]
        },
        "pieType": "pie",
        "reduceOptions": {
          "calcs": [ "lastNotNull" ],
          "fields": "",
          "values": false
        },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse" },
          "queryType": "sql",
          "rawQuery": true,
          "rawSql": "SELECT\n  toDateTime(0) AS time,\n  service,\n  count() AS value\nFROM analytics.fact_log\nWHERE $__timeFilter(event_ts)\nGROUP BY service\nORDER BY value DESC",
          "refId": "A"
        }
      ],
      "title": "서비스별 비중",
      "type": "piechart"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": [
    "clickhouse",
    "logs"
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Log Analytics Overview",
  "uid": "log-analytics",
  "version": 1,
  "weekStart": ""
}
